# Уровень A №3
class Shape:
    def area(self):
        raise NotImplementedError

class Rect(Shape):
    def __init__(self, width, height):
        self.width = width
        self.height = height

    def area(self):
        return self.width * self.height
rect = Rect(5, 10)
area = rect.area()
print(area)
